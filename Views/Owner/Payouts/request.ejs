<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <link rel="SHORTCUT ICON" href="<%=resources%>/g.assets/img/ico.ico">
    <link rel="ICON" href="<%=resources%>/g.assets/img/ico.ico">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
 
    <title>Exact request</title>

    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />
    <!-- Bootstrap core CSS     -->
    <link href="<%=resources%>/g.assets/semantic/semantic.min.css" rel="stylesheet">
    <link href="<%=resources%>/o.assets/css/bootstrap.min.css" rel="stylesheet" />
    <!--  Material Dashboard CSS    -->
    <link href="<%=resources%>/o.assets/css/material-dashboard.css?v=1.2.0" rel="stylesheet" />
    <!--  CSS for Demo Purpose, don't include it in your project     -->
    <link href="<%=resources%>/o.assets/css/demo.css" rel="stylesheet" />
    <!--     Fonts and icons     -->
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" rel="stylesheet">
    <link href='https://fonts.googleapis.com/css?family=Roboto:400,700,300|Material+Icons' rel='stylesheet' type='text/css'>
    <link href="https://fonts.googleapis.com/css?family=Saira+Semi+Condensed:100,200,300,400,500,600,700,800,900" rel="stylesheet">

</head>

<body class="preloading">
    <div class="wrapper">
        <div class="sidebar" data-color="purple" data-image="<%=resources%>/o.assets/img/sidebar-1.jpg">      
            <div class="logo">
                <a href="/" class="simple-text">
                    <img src="<%=resources%>/g.assets/img/tinylogo.png" alt="">
                </a>
            </div>
            <div class="sidebar-wrapper">
                <ul class="nav">
                    <li>
                        <a href="/admin/ab/dashboard">
                            <i class="material-icons">dashboard</i>
                            <p>Main</p>
                        </a>
                    </li> 
                    <li>
                        <a href="/admin/ab/products">
                            <i class="material-icons">assignment</i>
                            <p>Products</p>
                        </a>
                    </li>
                    <li class="divider">
                        Payouts
                    </li>
                    <li>
                        <a href="/admin/ab/payouts">
                            <i class="fa fa-credit-card-alt" aria-hidden="true"></i>
                            <p>New</p>
                        </a>
                    </li>  
                    <li>
                        <a href="/admin/ab/progress">
                            <i class="fa fa-credit-card" aria-hidden="true"></i>
                            <p>Processing</p>
                        </a>
                    </li>
                    <li>
                        <a href="/admin/ab/finished">
                            <i class="fa fa-history" aria-hidden="true"></i>
                            <p>Finished</p>
                        </a>
                    </li>
                    <li class="divider empty"></li>
                    <li>
                        <a href="/admin/ab/signout">
                            <i class="material-icons">https</i>
                            <p>Exit</p>
                        </a>
                    </li>              
                </ul>
            </div>
        </div>
        <div class="main-panel">
            <nav class="navbar navbar-transparent navbar-absolute">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>                          
                    </div>
                    <div class="collapse navbar-collapse">
                        <ul class="nav navbar-nav navbar-right">
                            <li>
                               
                            </li>
                            <li class="dropdown">
                              
                            </li>
                            <li>
                              
                            </li>
                        </ul>                   
                    </div>
                </div>
            </nav>
            <div class="content">
                <div class="container-fluid">
                    <div class="row">
                      <div class="col-md-12">
                            <div class="card">
                                <div class="card-header" data-background-color="purple">
                                    <h4 class="title">Exact payout</h4>
                                    <p class="category">
                                        ID <strong> <%=result.request.payoutId%> </strong> 
                                    </p>
                                </div>
                                <div class="card-content table-responsive">
                                    <div class="row">                                        
                                        <div class="col-sm-12 col-md-6 col-lg-6">
                                            <h5 class="hinfo">Seller information</h5>                                            
                                            <!--
                                            <div class="info">
                                                <i> Seller ID : </i>
                                                <strong> <%=result.request.requestor.uid%> </strong>
                                            </div>
                                            -->
                                            <div class="info">
                                                <i>First,last name :</i>
                                                <strong> 
                                                    <%=result.request.requestor.firstName%> 
                                                    <%=result.request.requestor.lastName%>
                                                </strong>
                                            </div>                                               
                                            <div class="info"> 
                                                <i>Contact email  :</i> 
                                                <strong> <%=result.request.requestor.email%> </strong>  
                                            </div>
                                            <div class="info">  
                                                <i>Contact phone :</i>
                                                <strong> <%=result.request.requestor.phone%> </strong> 
                                            </div>
                                            <div class="info">  
                                                <i>Paypal email :</i>
                                                <strong> <%=result.request.requestor.seller.paypalEmail%> </strong> 
                                            </div>
                                            <div class="info"> 
                                                 <i> Paypal available from :</i> 
                                                 <strong> <%=new Date(result.request.requestor.seller.paypalAccepted).toLocaleString()%> </strong> 
                                            </div>
                                        </div>    
                                        <div class="col-sm-12 col-md-6 col-lg-6">
                                            <h5 class="hinfo">Request information</h5>                                            
                                            <div class="info">
                                                <i>Amount (for payout) :</i>
                                                <strong> $ <%=(result.request.amount/100)%> </strong>
                                            </div>
                                            <div class="info">
                                                <i>Service Fee :</i>
                                                <strong> $ <%=(result.request.fee/100)%> </strong>
                                            </div>
                                            <div class="info">
                                                <i>Total :</i> 
                                                <strong> $ <%=((result.request.amount+result.request.fee)/100)%> </strong> 
                                            </div>
                                        </div>                                
                                    </div>
                                    <div id="accordion" role="tablist">
                                        <h5 class="hinfo margin-top">Related transaction information</h5>        
                                        <%result.request.billings.forEach((billing,i) => {%>
                                            <div class="card">
                                                <div class="card-header full" role="tab" id="heading<%=i%>" data-background-color="green">
                                                    <h5 class="mb-0">
                                                        <a class="collapsed" data-toggle="collapse" href="#collapse<%=i%>" aria-expanded="false" aria-controls="collapseTwo">
                                                            - <%=billing.type%>
                                                            &nbsp;&nbsp;&nbsp;&nbsp;  
                                                            <span>Amount : $<%=(billing.amount/100)%> </span>                                                                                                 
                                                            &nbsp;&nbsp;
                                                            <span> Fee : $<%=(billing.fee/100)%> </span>                                                                                                                 
                                                        </a>
                                                    </h5>
                                                </div>
                                                <div id="collapse<%=i%>" class="collapse" role="tabpanel" aria-labelledby="heading<%=i%>" data-parent="#accordion">
                                                <div class="card-body padding">
                                                    <div class="row">
                                                        <div class="col-sm-12 col-md-6">
                                                            <h4> Customer </h4>
                                                            <div class="info"> 
                                                                <i>First,last name :</i> 
                                                                <strong> <%=billing.customer.firstName%> <%=billing.customer.lastName%> </strong> 
                                                            </div>
                                                            <div class="info"> 
                                                                <i>Email :</i> 
                                                                <strong> <%=billing.customer.email%> </strong> 
                                                            </div>                                                            
                                                            <div class="info"> 
                                                                <i>Country :</i> 
                                                                <strong> <%=billing.customer.country%> </strong> 
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-12 col-md-6">
                                                            <h4> Payment information </h4>
                                                            <div class="info"> 
                                                                <i>Type :</i> 
                                                                <strong> <%=billing.type%> </strong> 
                                                            </div>
                                                            <div class="info"> 
                                                                <i>Amount :</i> 
                                                                <strong> $ <%=(billing.amount/100)%> </strong> 
                                                            </div>
                                                            <div class="info"> 
                                                                <i>Fee :</i> 
                                                                <strong> $ <%=(billing.fee/100)%> </strong> 
                                                            </div> 
                                                            <div class="info"> 
                                                                <i>Source ID (stripe) :</i> 
                                                                <strong> <%=billing.sourceId%> </strong> 
                                                            </div> 
                                                            <div class="info"> 
                                                                <i>Charge ID (stripe) :</i> 
                                                                <strong> <%=billing.chargeId%> </strong> 
                                                            </div> 
                                                        </div>
                                                        <div class="col-sm-12 col-md-6">
                                                            <h4 style="margin-top:85px;"> Charge information (stripe)</h4>
                                                            <div class="info"> 
                                                                <i>Charge ID :</i> 
                                                                <strong> <%=result.charges[i].id%> </strong> 
                                                            </div> 
                                                            <div class="info"> 
                                                                <i>Amount(smallest unit) :</i> 
                                                                <strong> <%=result.charges[i].amount%> </strong> 
                                                            </div> 
                                                            <div class="info"> 
                                                                <i>Captured :</i> 
                                                                <strong> <%=result.charges[i].captured%> </strong> 
                                                            </div> 
                                                            <div class="info"> 
                                                                <i>Created :</i> 
                                                                <strong> <%=new Date(result.charges[i].created*1000).toLocaleString()%> </strong> 
                                                            </div> 
                                                            <div class="info">
                                                                <i>Currency :</i>
                                                                <strong> <%=result.charges[i].currency%> </strong>
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-12 col-md-6">
                                                            <h4 style="margin-top:85px;"> Source information (stripe)</h4>
                                                            <div class="info"> 
                                                                <i>Source ID :</i> 
                                                                <strong> <%=result.charges[i].source.id%> </strong> 
                                                            </div>                                                           
                                                            <div class="info"> 
                                                                <i>Type :</i> 
                                                                <strong> <%=result.charges[i].source.type%> </strong> 
                                                            </div>
                                                            <div class="info"> 
                                                                <i>Created :</i> 
                                                                <strong> <%=new Date(result.charges[i].source.created*1000).toLocaleString()%> </strong> 
                                                            </div>                                                             
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <%})%>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>                    
                </div>
            </div>           
        </div>
    </div>
</body>
<!--   Core JS Files   -->
<script src="<%=resources%>/o.assets/js/jquery-3.2.1.min.js" type="text/javascript"></script>
<script src="<%=resources%>/o.assets/js/bootstrap.min.js" type="text/javascript"></script>
<script src="<%=resources%>/o.assets/js/material.min.js" type="text/javascript"></script>

<script src="<%=resources%>/o.assets/js/chartist.min.js"></script>

<script src="<%=resources%>/o.assets/js/arrive.min.js"></script>
<script src="<%=resources%>/o.assets/js/perfect-scrollbar.jquery.min.js"></script>
<script src="<%=resources%>/o.assets/js/bootstrap-notify.js"></script>

<script src="<%=resources%>/o.assets/js/material-dashboard.js?v=1.2.0"></script>

<script src="<%=resources%>/o.assets/js/demo.js"></script>


