<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <link rel="SHORTCUT ICON" href="<%=resources%>/g.assets/img/ico.ico">
    <link rel="ICON" href="<%=resources%>/g.assets/img/ico.ico">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
 
    <title>Dashboard</title>

    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />
    <!-- Bootstrap core CSS     -->
    <link href="<%=resources%>/o.assets/css/bootstrap.min.css" rel="stylesheet" />
    <!--  Material Dashboard CSS    -->
    <link href="<%=resources%>/o.assets/css/material-dashboard.css?v=1.2.0" rel="stylesheet" />
    <!--  CSS for Demo Purpose, don't include it in your project     -->
    <link href="<%=resources%>/o.assets/css/demo.css" rel="stylesheet" />
    <!--     Fonts and icons     -->
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" rel="stylesheet">
    <link href='https://fonts.googleapis.com/css?family=Roboto:400,700,300|Material+Icons' rel='stylesheet' type='text/css'>
    <link href="https://fonts.googleapis.com/css?family=Saira+Semi+Condensed:100,200,300,400,500,600,700,800,900" rel="stylesheet">
</head>

<body class="preloading">
    <div class="wrapper">
        <div class="sidebar" data-color="purple" data-image="<%=resources%>/o.assets/img/sidebar-1.jpg">      
            <div class="logo">
                <a href="/" class="simple-text">
                    <img src="<%=resources%>/g.assets/img/tinylogo.png" alt="">
                </a>
            </div>
            <div class="sidebar-wrapper">
                <%-include('./Common/menu',{ page : 1 })%>
            </div>
        </div>
        <div class="main-panel">
            <nav class="navbar navbar-transparent navbar-absolute">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>                          
                    </div>
                    <div class="collapse navbar-collapse">
                        <ul class="nav navbar-nav navbar-right">
                            <li>
                               
                            </li>
                            <li class="dropdown">
                              
                            </li>
                            <li>
                              
                            </li>
                        </ul>                   
                    </div>
                </div>
            </nav>
            <div class="content">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-3 col-md-6 col-sm-6">
                            <div class="card card-stats">
                                <div class="card-header" data-background-color="orange">
                                    <i class="material-icons">account_box</i>
                                </div>
                                <div class="card-content">
                                    <p class="category">Customers</p>
                                    <h3 class="title">
                                        <%=stats.users.customers%>
                                    </h3>
                                </div>
                                <div class="card-footer">
                                    <div class="stats">
                                         <i class="material-icons">date_range</i> All the time                                  
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-6 col-sm-6">
                            <div class="card card-stats">
                                <div class="card-header" data-background-color="red">
                                    <i class="material-icons">work</i>
                                </div>
                                <div class="card-content">
                                    <p class="category">Sellers</p>
                                    <h3 class="title"><%=stats.users.sellers%></h3>
                                </div>
                                <div class="card-footer">
                                    <div class="stats">
                                        <i class="material-icons">date_range</i> All the time
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-6 col-sm-6">
                            <div class="card card-stats">
                                <div class="card-header" data-background-color="green">
                                    <i class="material-icons">store</i>
                                </div>
                                <div class="card-content">
                                    <p class="category">Revenue</p>
                                    <h3 class="title">$<%=(stats.billing.day/100)%></h3>
                                </div>
                                <div class="card-footer">
                                    <div class="stats">
                                        <i class="material-icons">date_range</i> Last 24 Hours
                                    </div>
                                </div>
                            </div>
                        </div>                        
                        <div class="col-lg-3 col-md-6 col-sm-6">
                            <div class="card card-stats">
                                <div class="card-header" data-background-color="blue">
                                    <i class="material-icons">history</i>
                                </div>
                                <div class="card-content">
                                    <p class="category">Revenue History</p>
                                    <h3 class="title">$<%=(stats.billing.month/100)%></h3>
                                </div>
                                <div class="card-footer">
                                    <div class="stats">
                                        <i class="material-icons">date_range</i> Last month
                                    </div>
                                </div>
                            </div>
                        </div>                        
                    </div>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="card">
                                <div class="card-header card-chart" data-background-color="green">
                                    <div class="ct-chart" id="dailySalesChart"></div>
                                </div>
                                <div class="card-content">
                                    <h4 class="title">Daily Winnings</h4>
                                    <p class="category">
                                        For last 7 days
                                    </p>
                                </div>
                                <div class="card-footer">
                                    <div class="stats">
                                        <i class="material-icons">access_time</i> updated today
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="card">
                                <div class="card-header card-chart" data-background-color="orange">
                                    <div class="ct-chart" id="emailsSubscriptionChart"></div>
                                </div>
                                <div class="card-content">
                                    <h4 class="title">Customers payments</h4>
                                    <p class="category">Last Successful payments</p>
                                </div>
                                <div class="card-footer">
                                    <div class="stats">
                                        <i class="material-icons">access_time</i> updated today
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="card">
                                <div class="card-header card-chart" data-background-color="red">
                                    <div class="ct-chart" id="completedTasksChart"></div>
                                </div>
                                <div class="card-content">
                                    <h4 class="title">Created Products</h4>
                                    <p class="category">All created products</p>
                                </div>
                                <div class="card-footer">
                                    <div class="stats">
                                        <i class="material-icons">access_time</i> updated today
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6">
                            <div class="card">
                                <div class="card-header card-chart big" data-background-color="purple">
                                    <div class="ct-chart" id="monthUsersChart"></div>
                                </div>
                                <div class="card-content">
                                    <h4 class="title">Customers and sellers</h4>
                                    <p class="category">
                                        For last year
                                    </p>
                                </div>
                                <div class="card-footer">
                                    <div class="stats">
                                        <i class="material-icons">check</i> Including only verified
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="card">
                                <div class="card-header card-chart big" data-background-color="blue">
                                    <div class="ct-chart" id="monthWinningsChart"></div>
                                </div>
                                <div class="card-content">
                                    <h4 class="title">Winnings on auction</h4>
                                    <p class="category">For last year</p>
                                </div>
                                <div class="card-footer">
                                    <div class="stats">
                                        <i class="material-icons">attach_money</i> That was successfuly paid.    
                                    </div>
                                </div>
                            </div>
                        </div>                   
                    </div>
                  
                </div>
            </div>           
        </div>
    </div>
</body>
<!--   Core JS Files   -->
<script src="<%=resources%>/o.assets/js/jquery-3.2.1.min.js" type="text/javascript"></script>
<script src="<%=resources%>/o.assets/js/bootstrap.min.js" type="text/javascript"></script>
<script src="<%=resources%>/o.assets/js/material.min.js" type="text/javascript"></script>


<script src="<%=resources%>/o.assets/js/chartist.min.js"></script>

<script src="<%=resources%>/o.assets/js/arrive.min.js"></script>
<script src="<%=resources%>/o.assets/js/perfect-scrollbar.jquery.min.js"></script>
<script src="<%=resources%>/o.assets/js/bootstrap-notify.js"></script>

<script src="<%=resources%>/o.assets/js/material-dashboard.js?v=1.2.0"></script>

<script src="<%=resources%>/o.assets/js/demo.js"></script>
<script type="text/javascript">
    $(document).ready(function() {
        var myVar = <%-JSON.stringify(stats)%>;
        demo.initDashboardPageCharts(myVar);
    });
</script>

