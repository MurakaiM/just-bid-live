<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <link rel="SHORTCUT ICON" href="<%=domain%>/g.assets/img/ico.ico">
    <link rel="ICON" href="<%=domain%>/g.assets/img/ico.ico">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
 
    <title>Products approval</title>

    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />
    <!-- Bootstrap core CSS     -->
    <link href="<%=domain%>/g.assets/semantic/semantic.min.css" rel="stylesheet">
    <link href="<%=domain%>/o.assets/css/bootstrap.min.css" rel="stylesheet" />
    <!--  Material Dashboard CSS    -->
    <link href="<%=domain%>/o.assets/css/material-dashboard.css?v=1.2.0" rel="stylesheet" />
    <!--  CSS for Demo Purpose, don't include it in your project     -->
    <link href="<%=domain%>/o.assets/css/demo.css" rel="stylesheet" />
    <!--     Fonts and icons     -->
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" rel="stylesheet">
    <link href='https://fonts.googleapis.com/css?family=Roboto:400,700,300|Material+Icons' rel='stylesheet' type='text/css'>
    <link href="https://fonts.googleapis.com/css?family=Saira+Semi+Condensed:100,200,300,400,500,600,700,800,900" rel="stylesheet">
</head>

<body class="preloading">
    <div class="wrapper">
        <div class="sidebar" data-color="purple" data-image="<%=domain%>/o.assets/img/sidebar-1.jpg">      
            <div class="logo">
                <a href="/" class="simple-text">
                    <img src="<%=domain%>/g.assets/img/tinylogo.png" alt="">
                </a>
            </div>
            <div class="sidebar-wrapper">
                <ul class="nav">
                    <li>
                        <a href="/admin/ab/dashboard">
                            <i class="material-icons">dashboard</i>
                            <p>Main</p>
                        </a>
                    </li> 
                    <li class="active">
                        <a href="/admin/ab/products">
                            <i class="material-icons">assignment</i>
                            <p>Products</p>
                        </a>
                    </li>
                    <li class="divider">
                        Payouts
                    </li>
                    <li>
                        <a href="/admin/ab/payouts">
                            <i class="fa fa-credit-card-alt" aria-hidden="true"></i>
                            <p>New</p>
                        </a>
                    </li>  
                    <li>
                        <a href="/admin/ab/payouts">
                            <i class="fa fa-credit-card" aria-hidden="true"></i>
                            <p>Processing</p>
                        </a>
                    </li>
                    <li>
                        <a href="/admin/ab/finished">
                            <i class="fa fa-history" aria-hidden="true"></i>
                            <p>Finished</p>
                        </a>
                    </li>
                    <li class="divider empty"></li>
                    <li>
                        <a href="/admin/ab/signout">
                            <i class="material-icons">https</i>
                            <p>Exit</p>
                        </a>
                    </li>              
                </ul>
            </div>
        </div>
        <div class="main-panel">
            <nav class="navbar navbar-transparent navbar-absolute">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>                          
                    </div>
                    <div class="collapse navbar-collapse">
                        <ul class="nav navbar-nav navbar-right">
                            <li>
                               
                            </li>
                            <li class="dropdown">
                              
                            </li>
                            <li>
                              
                            </li>
                        </ul>                   
                    </div>
                </div>
            </nav>
            <div class="content">
                <div class="container-fluid">
                    <div class="row">
                      <div class="col-md-12">
                            <div class="card">
                                <div class="card-header" data-background-color="purple">
                                    <h4 class="title">Products for review</h4>
                                    <p class="category">Accept or decline application for product createing</p>
                                </div>
                                <div class="card-content table-responsive">
                                    <table class="table">
                                        <thead class="text-primary">
                                            <th>Title</th>
                                            <th>Link</th>
                                            <th>Seller</th>
                                            <th>Application created</th>
                                            <th>Actions</th>
                                        </thead>
                                        <tbody>
                                            <%if(products.length == 0){%>
                                                <tr>
                                                    <td>
                                                        There are no products for review
                                                    </td>
                                                </tr>
                                            <%}%>

                                            <%products.forEach( product => {%>  
                                                <tr data-id="<%=product.prUid%>">
                                                    <td><%=product.prTitle%></td>
                                                    <td>
                                                        <a href="<%=domain%>/admin/ab/product/<%=product.prUid%>" target="blank"> 
                                                            <b>Product page</b> 
                                                        </a>
                                                    </td>
                                                    <td><%=product.creator.firstName%> <%=product.creator.lastName%></td>
                                                    <td><%=new Date(product.createdAt).toLocaleString()%></td>
                                                    <td>
                                                        <button rel="tooltip" title="" class="ui button btn btn-success btn-simple btn-xs approve" data-original-title="Approve product">
                                                                <i class="material-icons">check</i>
                                                            <div class="ripple-container"></div>
                                                        </button>
                                                        <button  rel="tooltip" title="" class="ui button btn btn-danger btn-simple btn-xs decline" data-original-title="Decline product">
                                                                <i class="material-icons">clear</i>
                                                            <div class="ripple-container"></div>
                                                        </button>
                                                    </td>
                                                </tr>
                                            <%})%>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>                    
                </div>
            </div>           
        </div>
    </div>
</body>
<!--   Core JS Files   -->
<script src="<%=domain%>/o.assets/js/jquery-3.2.1.min.js" type="text/javascript"></script>
<script src="<%=domain%>/o.assets/js/bootstrap.min.js" type="text/javascript"></script>
<script src="<%=domain%>/o.assets/js/material.min.js" type="text/javascript"></script>

<script src="<%=domain%>/o.assets/js/chartist.min.js"></script>

<script src="<%=domain%>/o.assets/js/arrive.min.js"></script>
<script src="<%=domain%>/o.assets/js/perfect-scrollbar.jquery.min.js"></script>
<script src="<%=domain%>/o.assets/js/bootstrap-notify.js"></script>

<script src="<%=domain%>/o.assets/js/material-dashboard.js?v=1.2.0"></script>

<script src="<%=domain%>/o.assets/js/demo.js"></script>
<script src="<%=domain%>/o.assets/js/approval.js"></script>

